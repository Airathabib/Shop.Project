{
    "env": {
        "browser": true,
        "es2021": true
    },
		"settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
				"plugin:react/jsx-runtime",
				"airbnb",
				 "import",
				"plugin:react-hooks/recommended",
        "plugin:react/recommended",
				"prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
    }
}
